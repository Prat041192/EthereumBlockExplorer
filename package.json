{
  "name": "Ethereum-Fraud-Explorer",
  "version": "0.0.1",
  "description": "A block explorer Front End to connect to the etherium parity client for visualisation of Etherium chain and possible frauds ",
  "author": "IOSL group, TU-Berlin",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"RUN_IMPORTER=1 npm run start:server:dev\" \"npm run start:client:dev\" ",
    "start:importer": "concurrently --kill-others \"RUN_IMPORTER=1 npm run start:server:dev\"",
    "start:server:dev": "better-npm-run start-server-dev",
    "start:client:dev": "webpack-dev-server --inline --progress --config client/build/webpack.dev.conf.js",
    "lint": "standard --fix",
    "build": "node client/build/build.js"
  },
  "betterScripts": {
    "start-server-dev": {
      "command": "nodemon ./scripts/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
